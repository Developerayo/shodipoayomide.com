{
  "version": 2,
  "name": "Shodipo Ayomide",
  "alias": "shodipoayomide.com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "src": "^/public/static/(.*)",
      "headers": { "cache-control": "public,max-age=0, no-cache" }
    },
    {
      "src": "^/(.*).(css|js)",
       "headers": { "cache-control": "public,max-age=0, no-cache" }
    },
    {
      "src": "^/(.*).html",
      "headers": { "cache-control": "public,max-age=0, no-cache" }
    }
  ]
}
